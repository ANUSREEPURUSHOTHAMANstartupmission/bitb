				  // for all the TR's
				  if(keys[j] == "data"){
					//if it is the data value
					dataTest = trs[i].children[j].innerHTML;
					//grab the value as a string 
					dataTest = parseFloat(dataTest);
					//convert it to a number
					obj[keys[j]] = dataTest;
					//add it to obj
