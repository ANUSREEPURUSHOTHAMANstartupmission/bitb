		 var  dataTest = "";//a place holder to convert to a number (for values)
	  
		for (; i < trl; i++) {
		  //loop through the table rows
			if (i == 0) {
			  //if its the first row i.e. TH's
				for (; j < trs[i].children.length + 1; j++) {
				  //loop through all the TH's + one more for color
				  if(j == 2)
					//if it is color
