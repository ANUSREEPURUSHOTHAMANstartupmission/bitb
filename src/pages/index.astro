						ctx.closePath();
						x = mouseX - centerLeft;
						y = mouseY - centerTop;
	
						if (ctx.isPointInPath) {
							if (ctx.isPointInPath(mouseX - centerLeft, mouseY - centerTop)) {
								ctx.restore();
								return {
